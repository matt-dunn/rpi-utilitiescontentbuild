{
    "name": "rpi/utilities/contentbuild",
    "type": "library",
    "description": "Content Builder",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "yui/yuicompressor",
                "version": "2.4.7",
                "dist": {
                    "url": "https://github.com/downloads/yui/yuicompressor/yuicompressor-2.4.7.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ulrichsg/getopt-php": "dev-master",
        "yui/yuicompressor": "2.4.7"
    },
    "require-dev": {
        "phing/phing": "dev-master",
        "phpunit/phpunit": "3.7.*"
    },
    "target-dir": "RPI/Utilities/ContentBuild",
    "autoload": {
        "psr-0": {
            "RPI\\Utilities\\ContentBuild": "Src"
        }
    }
}