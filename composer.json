{
    "name": "rpi/utilities-contentbuild",
    "type": "library",
    "description": "CSS/JS Content Builder",
    "license": "proprietary",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "yui/yuicompressor",
                "version": "2.4.7",
                "dist": {
                    "url": "https://github.com/downloads/yui/yuicompressor/yuicompressor-2.4.7.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "composer",
            "url": "http://packages.red-pixel.co.uk/"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "rpi/console": "dev-master"
    },
    "require-dev": {
        "ulrichsg/getopt-php": "dev-master",
        "yui/yuicompressor": "2.4.7",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "RPI\\Utilities\\ContentBuild": "Src/"
        }
    },
    "bin": [
        "bin/contentbuild"
    ],
    "minimum-stability": "dev"
}