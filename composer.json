{
    "name": "rpi/content-build",
    "type": "library",
    "description": "Content Builder",
     "repositories": [
        {
            "type": "package",
            "package": {
                "name": "yui/yuicompressor",
                "version": "2.4.7",
                "dist": {
                    "url": "https://github.com/downloads/yui/yuicompressor/yuicompressor-2.4.7.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "composer",
            "url": "http://packages.red-pixel.co.uk"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ulrichsg/getopt-php": "dev-master",
        "yui/yuicompressor": "2.4.*",
        "rpi/framework": "dev-master",
        "rpi/components/carousel": "dev-master"
    },
    "require-dev": {
        "phing/phing": "dev-master",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "RPI": "src"
        }
    }
}